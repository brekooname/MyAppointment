
@model IEnumerable<MyAppointment.Models.Techline>

@{

    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@if (User.Identity.IsAuthenticated)
{
    <div class="container">
        <div class="row">
            <div class="col-lg-12">
                <h2> <i class="fa fa-users fa-2x"></i> Tech-Line-Support-For @DateTime.Now.Year</h2>
            </div>
        </div>
        <div class="row">
            <div class="table-responsive">
                <table id="warranty" class="table table-bordered table-hover">
                    <thead>
                        <tr>
                            <th>Brand Name</th>
                            <th>Account Number</th>
                            <th>Phone Number</th>
                        </tr>
                    </thead>
                    <tbody>
                    </tbody>
                </table>
            </div>
        </div>
    </div>
}
else
{
    <div class="container">
        <a class="btn btn-danger" asp-action="Login" asp-controller="Account">Sign In</a> to assets Customer Lists!!!
    </div>
}

@section scripts {
    <script>
        $(document).ready(function () {
            var table = $("#warranty").DataTable({
                ajax: {
                    url: "/warranty/getall"
                  
                },
                columns: [
                    { data: "companyName"},
                    { data: "Details" },
                    { data: "phoneNumber" }

                ]
            });
        });
    </script>

}